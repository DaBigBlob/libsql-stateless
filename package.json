{
  "name": "libsql-stateless",
  "version": "2.8.3",
  "description": "thin libSQL stateless http driver for TypeScript and JavaScript",
  "homepage": "https://github.com/DaBigBlob/libsql-stateless#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaBigBlob/libsql-stateless.git"
  },
  "bugs": {
    "url": "https://github.com/DaBigBlob/libsql-stateless/issues",
    "email": "localboxcrox@gmail.com"
  },
  "author": {
    "name": "LocalBox Crox",
    "email": "localboxcrox@gmail.com"
  },
  "license": "MIT",
  "main": "./dist/main.js",
  "type": "module",
  "types": "./dist/main.d.ts",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    }
  },
  "devDependencies": {
    "semantic-release": "^23.0.8",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "scripts": {
    "prepublishOnly:old": "npm run build",
    "prebuild:old": "rm -rf ./dist",
    "build:old": "tsc -p tsconfig.json",
    "typecheck:old": "tsc --noEmit",
    "test:old": "bun run _tests/test2.ts",
    "perf:old": "bun run _tests/perf.ts",
    "clean:old": "npm run prebuild",
    "prod:old": "npm publish && npm run clean",
    "build": "tsup",
    "dev": "tsup --watch --clean=false",
    "test": "vitest",
    "test:watch": "vitest watch",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "libsql",
    "database",
    "sqlite",
    "serverless",
    "vercel",
    "netlify",
    "lambda",
    "http",
    "https",
    "webapi",
    "cloudflare-workers",
    "cloudflare-pages",
    "edge"
  ]
}
